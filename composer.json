{
    "name": "xchanming/elasticsearch",
    "description": "Elasticsearch for Shopware",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Shopware\\Elasticsearch\\": ""
        },
        "exclude-from-classmap": [
            "/Test/"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "support": {
        "issues": "https://github.com/xchanming/xchanming/issues/",
        "forum": "https://forum.xchanming.com",
        "wiki": "https://developer.xchanming.com",
        "docs": "https://developer.xchanming.com",
        "chat": "https://slack.xchanming.com"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.7.x-dev",
            "dev-trunk": "6.7.x-dev"
        }
    },
    "minimum-stability": "stable",
    "require": {
        "php": "~8.2.0 || ~8.3.0 || ~8.4.0",
        "ext-curl": "*",
        "async-aws/core": "^1.22",
        "doctrine/dbal": "^4.2",
        "monolog/monolog": "^3.3.1",
        "opensearch-project/opensearch-php": "^2.3.1",
        "xchanming/core": "*",
        "shyim/opensearch-php-dsl": "^1.0.5",
        "symfony/http-foundation": "~7.2.0",
        "symfony/messenger": "~7.2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5.38"
    }
}
